{
    "name": "MySuperGenPass",
    "version": "0.1",
    "manifest_version": 2,
    "description": "My vision of a perfect SuperGenPass extension",
    "browser_action": {
        "default_icon": "icon128.png",
        "default_title": "SuperGenPass",
        "default_popup": "popup.html"
    },

    "permissions": [
        "tabs"
    ],

    "icons": { "16": "icon16.png",
               "48": "icon48.png",
               "128": "icon128.png" }
}
